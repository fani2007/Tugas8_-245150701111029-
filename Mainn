public class Mainn {
    public static void main(String[] args) {

        Invoice invoice1 = new Invoice("Sabun", 2, 5000);
        Invoice invoice2 = new Invoice("Shampoo", 1, 15000);
        Invoice invoice3 = new Invoice("Sikat Gigi", 3, 4000);

        Invoice[] daftarBelanja = {invoice1, invoice2, invoice3};

        Employee karyawan = new Employee(101, "Tiara", 3000000, daftarBelanja);

        karyawan.displayEmployeeDetails();
    }
}
